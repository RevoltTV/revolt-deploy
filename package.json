  },
  "homepage": "https://github.com/RevoltTV/revolt-deploy#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "cross-env": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-config-revolt": "^1.2.0",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "aws-sdk": "^2.5.1",
